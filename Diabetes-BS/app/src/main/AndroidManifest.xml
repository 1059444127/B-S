<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="diabetes.czk.com">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/main_activity_style">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".setting.SettingActvity" />

        <activity android:name=".setting.LanguageSettingActivity" />

        <activity android:name=".signin.SignActivity" />

        <activity android:name=".AddValueActivity" />
    </application>

</manifest>