<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--Blue_Theme-->
    <item
        android:maxLevel="0"
        android:minLevel="0">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/blue_color_light" />
        </shape>
    </item>
    <!--Pink_Theme-->
    <item
        android:maxLevel="1"
        android:minLevel="1">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/pink_color_light" />
        </shape>
    </item>
    <!--Purple_Theme-->
    <item
        android:maxLevel="2"
        android:minLevel="2">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/purple_color_light" />
        </shape>
    </item>
    <!--Black_Theme-->
    <item
        android:maxLevel="3"
        android:minLevel="3">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/black_color_light" />
        </shape>
    </item>
</level-list>