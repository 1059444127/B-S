<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="start_angle" format="float" />
    <declare-styleable name="CircleProgressBar">
        <attr name="background_color" format="color|reference" />
        <attr name="progress_color" format="color|reference" />
        <attr name="progress_background_color" format="color|reference" />
        <attr name="progress_width" format="dimension|reference" />
        <attr name="start_angle" />
        <attr name="value" format="float" />
        <attr name="max_value" format="float" />
    </declare-styleable>
    <declare-styleable name="PieChartView">
        <attr name="start_angle" />
    </declare-styleable>
    <declare-styleable name="LineChartView">
        <attr name="line_color" format="color|reference" />
        <attr name="xline_color" format="color|reference" />
        <attr name="yline_color" format="color|reference" />
        <attr name="line_width" format="dimension|reference" />
        <attr name="xline_width" format="dimension|reference" />
        <attr name="yline_width" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="MeterView">
        <attr name="title_visible">
            <enum name="visble" value="0"/>
            <enum name="invisble" value="4"/>
            <enum name="gone" value="8"/>
        </attr>
    </declare-styleable>
</resources>